{
    "name": "amatsuka/himekawa",
    "description": "himekawa",
    "keywords": [
        "himekawa"
    ],
    "license": "BSD-2-Clause",
    "type": "project",
    "require": {
        "php": ">=7.4",
        "doctrine/dbal": "^2.6",
        "erusev/parsedown": "^1.7",
        "fideloper/proxy": "^4.0",
        "jacobbennett/laravel-http2serverpush": "^4.0",
        "ksmz/json": "^0.1",
        "ksmz/nana-laravel": "0.*",
        "laravel-notification-channels/telegram": "^0.1.1",
        "laravel/framework": "^6.2",
        "laravel/telescope": "^2.0",
        "laravel/tinker": "^2.0",
        "predis/predis": "^1.1",
        "spatie/laravel-feed": "2.4.3",
        "spatie/laravel-slack-slash-command": "^1.9"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.2",
        "barryvdh/laravel-ide-helper": "^2.6",
        "facade/ignition": "^1.4",
        "fzaninotto/faker": "^1.4",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^3.0",
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "files": [
            "yuki/helpers.php"
        ],
        "psr-4": {
            "himekawa\\": "app/",
            "yuki\\": "yuki/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
                "barryvdh/laravel-debugbar"
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi",
            "@php artisan generate:helpers --composer"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
