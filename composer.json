{
    "name": "amatsuka/himekawa",
    "description": "himekawa",
    "keywords": [
        "himekawa"
    ],
    "license": "BSD-2-Clause",
    "type": "project",
    "require": {
        "php": ">=7.1.3",
        "cakephp/chronos": "^1.1",
        "doctrine/dbal": "^2.6",
        "fideloper/proxy": "~4.0",
        "jacobbennett/laravel-http2serverpush": "^2.0",
        "ksmz/json": "^0.1.0",
        "ksmz/nana-laravel": "^0.0.1",
        "laravel-notification-channels/telegram": "^0.0.4",
        "laravel/framework": "5.6.*",
        "laravel/tinker": "~1.0",
        "predis/predis": "^1.1",
        "spatie/laravel-feed": "^2.0",
        "symfony/polyfill-php73": "^1.8"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.1",
        "barryvdh/laravel-ide-helper": "^2.4",
        "filp/whoops": "~2.0",
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "~1.0",
        "nunomaduro/collision": "~2.0",
        "phpunit/phpunit": "~7.0"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "files": [
            "yuki/helpers.php"
        ],
        "psr-4": {
            "himekawa\\": "app/",
            "yuki\\": "yuki/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover",
            "@php artisan generate:helpers --composer"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
